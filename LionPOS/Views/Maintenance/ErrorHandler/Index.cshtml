                        object objException = new LionUtilities.ConversionUtilitise().StringToObject(Model.exception);

    }
    <header>
        <div class="header-wrapper">

            @*@Html.Raw(@Session["ErrorMessage"].ToString())*@
            @*<img id="logo" src="~/Content/ContentMaintenance/img/logo.png" alt="Mandi logo">*@

            @if(objException.ToString().ToLower().Contains("foreign key"))
                {
                <h1><span></span>Delete failed!</h1>

                <h2> Some data can not be removed due to they are dependent on other data.To remove this failed data make sure you remove dependent data first.Example: Employee can not delete if he has any record of salary paid in past. </h2>
                <h2>Please Contact Developers If you have query.</h2>
                }
                else if (objException.ToString().ToLower().Contains("unique"))
                {
                <h1><span></span>Adding or Updating failed!</h1>

                <h2>Data transaction can not be performed due to the data you are trying to add or modify is duplication in system.Example: India can not be added twice. </h2>
                <h2>Please Contact Developers If you have query.</h2>
                }
                else if (objException.ToString().ToLower().Contains("provide"))
                {
                <h1><span></span>Database connection Failed!</h1>

                <h2>Some error occured connection database.<br />Possible reasones : <br />1) Database connection failed <br />2)Server not available<br /> 3)Database is busy due to heavy load.</h2>
                <h2>Please Contact Developers If you have query.</h2>
                }
                else
                {
                <h1><span></span>Something is going wrong</h1>
                <h2>Please Contact Developers</h2>
                <h2> To Get Support & Help Please Refer Following Get In Touch </h2>
                }

        </div>
    </header>
    <div class="wrapper">
        <main>

            @*<div id="countdown" data-countdown="2016/01/06">
